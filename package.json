{
  "name": "gatsby-starter",
  "version": "1.0.0",
  "description": "Gatsby 1.0 starter",
  "author": "fabien0102 <fabien0102@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "export-build": "gatsby build --prefix-paths",
    "tsbuild": "tsc",
    "lint": "npm run lint:ts && npm run lint:md && npm run lint:js && npm run lint:css",
    "lint:ts": "tslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "lint:md": "remark .",
    "lint:js": "xo",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch",
    "graphql-types": "gql-gen --url http://localhost:8000/___graphql --template typescript --out ./src/graphql-types.d.ts",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o public/docs",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "dependencies": {
    "@types/lodash": "^4.14.116",
    "@types/react-dom": "^16.0.2",
    "@types/react-helmet": "5.0.0",
    "@types/react-redux": "^6.0.6",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-styled-components": "^1.5.1",
    "change-case": "^3.0.1",
    "cloudinary-react": "^1.0.6",
    "codeclimate-test-reporter": "^0.4.1",
    "disqus-react": "^1.0.5",
    "firebase": "4.13.0",
    "gatsby": "1.9.277",
    "gatsby-link": "1.6.46",
    "gatsby-plugin-glamor": "1.6.13",
    "gatsby-plugin-google-analytics": "1.0.31",
    "gatsby-plugin-manifest": "1.0.27",
    "gatsby-plugin-sharp": "1.6.48",
    "gatsby-plugin-styled-components": "^2.0.11",
    "gatsby-plugin-typescript": "1.4.20",
    "gatsby-remark-autolink-headers": "1.4.19",
    "gatsby-remark-copy-linked-files": "1.5.37",
    "gatsby-remark-images": "1.5.67",
    "gatsby-remark-prismjs": "2.0.5",
    "gatsby-source-filesystem": "1.5.39",
    "gatsby-transformer-json": "1.0.20",
    "gatsby-transformer-remark": "1.7.44",
    "gatsby-transformer-sharp": "1.6.27",
    "graphql-code-generator": "^0.5.2",
    "gray-matter": "^2.1.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "5.0.3",
    "react-redux": "^5.0.6",
    "redux-devtools-extension": "^2.13.2",
    "slash": "1.0.0",
    "styled-components": "^3.4.2",
    "ts-loader": "^2.3.7",
    "typescript": "^2.9.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.14",
    "@storybook/addon-knobs": "^3.2.14",
    "@storybook/addon-notes": "^3.2.10",
    "@storybook/addon-options": "^3.2.14",
    "@storybook/react": "^3.2.14",
    "@types/enzyme": "^2.8.9",
    "@types/jest": "^19.2.2",
    "@types/js-cookie": "^2.1.0",
    "@types/node": "^7.0.15",
    "@types/react": "^16.4.9",
    "@types/react-test-renderer": "^16.0.0",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-knobs": "^3.2.0",
    "@types/storybook__addon-notes": "^3.0.1",
    "@types/storybook__addon-options": "^3.2.0",
    "@types/storybook__react": "^3.0.5",
    "@types/webpack-env": "^1.13.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint-config-xo-react": "0.11.1",
    "eslint-plugin-react": "6.10.3",
    "flat": "^2.0.1",
    "husky": "^0.13.3",
    "jest": "19.0.2",
    "lint-staged": "3.4.0",
    "mkdirp": "^0.5.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "16",
    "remark-cli": "^3.0.1",
    "remark-preset-lint-recommended": "^2.0.0",
    "storybook-readme": "^3.0.6",
    "ts-jest": "19.0.8",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "xo": "^0.18.1"
  },
  "keywords": [
    "gatsby",
    "starter"
  ],
  "jest": {
    "globals": {
      "__LINK_PREFIX__": "",
      "__PREFIX_LINKS__": ""
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/templates/",
      "/.cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "xo --fix",
      "git add"
    ]
  },
  "xo": {
    "extends": "xo-react/space",
    "space": true,
    "rules": {
      "no-case-declarations": 0,
      "default-case": 0
    }
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
